{
  "name": "react-text-mask-hoc",
  "version": "0.10.6",
  "description": "A higher-order text-mask component for React and React Native.",
  "author": "Vlad Zhukov (http://github.com/Vlad-Zhukov)",
  "license": "MIT",
  "repository": "Vlad-Zhukov/react-text-mask-hoc",
  "bugs": {
    "url": "https://github.com/Vlad-Zhukov/react-text-mask-hoc/issues"
  },
  "main": "./dist/react-text-mask-hoc.cjs.js",
  "module": "./dist/react-text-mask-hoc.esm.js",
  "scripts": {
    "build": "rimraf dist/ && cross-env NODE_ENV=production node ./rollup.js",
    "test": "jest",
    "lint": "eslint \"**/*.js\"",
    "prettier": "prettier \"**/*.{js,md}\" --write --loglevel warn",
    "prepublishOnly": "yarn lint && yarn build && yarn test"
  },
  "dependencies": {
    "hoist-non-react-statics": "^2.3.1",
    "prop-types": "^15.6.0",
    "react-shallow-equal": "^0.1.1"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-beta.2",
    "babel-eslint": "^8.0.1",
    "babel-plugin-external-helpers": "^7.0.0-beta.2",
    "babel-preset-env": "^2.0.0-beta.2",
    "babel-preset-react": "^7.0.0-beta.2",
    "babel-preset-stage-2": "^7.0.0-beta.2",
    "cross-env": "^5.1.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.24.1",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^21.3.0-beta.2",
    "prettier": "^1.14.3",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.4",
    "rollup-plugin-node-resolve": "^3.0.0",
    "text-mask-addons": "^3.7.0",
    "text-mask-core": "^5.0.1"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "keywords": [
    "react",
    "react-native",
    "text-mask",
    "react-text-mask",
    "react-component",
    "react hoc",
    "hoc",
    "text mask",
    "input mask",
    "string mask",
    "input formatting",
    "text formatting",
    "string formatting"
  ],
  "files": [
    "src",
    "dist",
    "ReactNative.js"
  ],
  "jest": {
    "roots": [
      "<rootDir>/__tests__/"
    ],
    "transformIgnorePatterns": [
      "/node_modules((?!text-mask-core).)*$/"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "trailingComma": "es5",
    "singleQuote": true,
    "bracketSpacing": false,
    "jsxBracketSameLine": false,
    "proseWrap": "always"
  }
}
